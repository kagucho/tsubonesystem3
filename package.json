{
  "name": "tsubonesystem3",
  "version": "0.0.0",
  "description": "A Web service used in Kagucho, a club in Tokyo University of Science",
  "main": "see_readme.js",
  "scripts": {
    "fmt": "npm run webapp-eslint-fix & npm run gofmt; wait",
    "gofmt": "gofmt -w .",
    "goinstall": "go install ./frontend/tsubonesystem3",
    "goinstall-fcgi": "go install ./frontend/tsubonesystem3_fcgi",
    "golint": "golint ./...",
    "gotest": "go test ./...",
    "install": "npm run webapp-webpack & npm run goinstall; wait",
    "install-fcgi": "npm run webapp-webpack & npm run goinstall-fcgi; wait",
    "lint": "npm run webapp-eslint & TSUBONESYSTEM_URL=`go run ./frontend/tsubonesystem3_resolve` npm run vnu & npm run golint; wait",
    "postlint": "npm stop",
    "posttest": "npm stop",
    "prelint": "npm restart",
    "pretest": "npm restart",
    "restart": "npm stop && npm start",
    "start": "npm run tsubonesystem3 & npm run webapp-webpack-watch &",
    "stop": "ps -o pid,cmd | grep -e \"webpack --output-path $GOPATH/share/tsubonesystem3/public --watch\\$\" -e \"^\\s*[[:digit:]]* $GOPATH/bin/tsubonesystem3\\$\" | while read -r PID _; do kill $PID; done",
    "test": "TSUBONESYSTEM_URL=`go run ./frontend/tsubonesystem3_resolve` npm run webapp-karma-auto & npm run gotest; wait",
    "tsubonesystem3": "\"$GOPATH/bin/tsubonesystem3\"",
    "vnu": "java -jar node_modules/vnu-jar/build/dist/vnu.jar \"$TSUBONESYSTEM_URL\" \"$TSUBONESYSTEM_URL/license\" \"$TSUBONESYSTEM_URL/private\" \"$TSUBONESYSTEM_URL/private?_escaped_fragment_=\"",
    "webapp-eslint": "npm run webapp-eslint-build & npm run webapp-eslint-src & npm run webapp-eslint-test; wait",
    "webapp-eslint-build": "cd webapp; eslint --env node --ignore-pattern src .",
    "webapp-eslint-fix": "npm run webapp-eslint-fix-build & npm run webapp-eslint-fix-src & npm run webapp-eslint-fix-test; wait",
    "webapp-eslint-fix-build": "npm run webapp-eslint-build -- --fix",
    "webapp-eslint-fix-src": "npm run webapp-eslint-src -- --fix",
    "webapp-eslint-fix-test": "npm run webapp-eslint-test -- --fix",
    "webapp-eslint-src": "cd webapp; eslint --env browser,es6,jquery --ignore-pattern 'src/**/*_test.js' src",
    "webapp-eslint-test": "cd webapp; eslint --env browser,es6,jquery,mocha 'src/**/*_test.js'",
    "webapp-jsdoc": "cd webapp; jsdoc -r -R ../README.md src",
    "webapp-karma": "cd webapp; karma start",
    "webapp-karma-auto": "cd webapp; karma start --browsers PhantomJS --single-run",
    "webapp-webpack": "cd webapp; webpack --output-path \"$GOPATH/share/tsubonesystem3/public\"",
    "webapp-webpack-watch": "cd webapp; webpack --output-path \"$GOPATH/share/tsubonesystem3/public\" --watch || test $?=143"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kagucho/tsubonesystem3.git"
  },
  "author": "Akihiko Odaki <akihiko.odaki.4i@stu.hosei.ac.jp>",
  "license": "AGPL-3.0+",
  "bugs": {
    "url": "https://github.com/kagucho/tsubonesystem3/issues"
  },
  "homepage": "https://github.com/kagucho/tsubonesystem3#readme",
  "dependencies": {
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2016": "^6.16.0",
    "css-loader": "^0.26.1",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.4",
    "postcss-cssnext": "^2.9.0",
    "postcss-loader": "^1.2.0",
    "style-loader": "^0.13.1",
    "stylelint": "^7.6.0",
    "stylelint-config-standard": "^15.0.0",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.0"
  },
  "devDependencies": {
    "babel-polyfill": "^6.22.0",
    "eslint": "^3.10.2",
    "eslint-plugin-nodeca": "^1.0.3",
    "jquery": "^3.1.1",
    "jsdoc": "^3.4.3",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-edge-launcher": "^0.2.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.1",
    "mithril": "^0.2.5",
    "mocha": "^3.1.2",
    "vnu-jar": "^17.1.0"
  }
}
